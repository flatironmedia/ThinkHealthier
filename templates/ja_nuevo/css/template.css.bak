/* Health AZ & AZ Alphabet */

.view-healthaz .t3-mainbody,
.view-azalphabet .t3-mainbody {
    background: #f2f2f2;
    clear: both;
    margin-top: 20px;
}

.view-healthaz .t3-mainbody .t3-content,
.view-azalphabet .t3-mainbody .t3-content {
    margin-top: 0;
    padding-top: 10px;
    padding-left: 10px;
    padding-right: 0;
}

.healthaz-label-heading,
.azalphabet-label-heading {
	background-color: #fff;
	padding: 20px 25px;
}

.healthaz-label-heading .healthaz-text,
.azalphabet-label-heading .azalphabet-text {
	float: left;
	font-size: 15px;
	display: inline-block;
	width: 100%;
	max-width: 720px;
	padding: 10px 0px;
}

.healthaz-label-heading .healthaz-adam-img,
.azalphabet-label-heading .azalphabet-adam-img {
	display: inline-block;
	width: 100%;
	max-width: 150px;
	border-left: 1px solid #e3e3e3;
	margin-left: 20px;
	padding: 5px 0px 5px 20px;
}

.healthaz-categories-all {
	background-color: #fff;
	padding: 10px;
	float: left;
	margin-top: 10px;
	padding-right: 0px;
}

.healthaz-categories-all h4 {
	color: #444450;
	font-size: 24px;
	margin: 5px 0px 20px;
}

.healthaz-categories-all .healthaz-category {
	width: 100%;
	float: left;
	max-width: 303px;
	display: inline-block;
	margin-right: 10px;
	margin-bottom: 8px;
	vertical-align: top;
	background-color: #e7e7e7;
}

.healthaz-category .healthaz-category-title {
	background: #39645a;
	padding: 4px 10px;
	color: #fff;
	font-size: 24px;
}

.healthaz-category .healthaz-category-title::before {
	content: "";
	border: 17.5px solid transparent;
	border-bottom: 15px solid #39645a;
	margin-top: -35px;
	margin-left: 5px;
	position: absolute;
}

.healthaz-category .healthaz-article-most-viewed {
	color: #454e4c;
	font-size: 16px;
	font-weight: 700;
	padding-bottom: 5px;
}

.healthaz-content-without-title {
	padding: 15px 20px 20px 20px;
}

.healthaz-content-without-title .healthaz-article-title > a {
	color: #39645a;
	font-size: 16px;
}

.healthaz-content-without-title .healthaz-article-title > a:hover {
	color: #60bca6;
}

.view-azalphabet .azalphabet-letters {
	clear: both;
	border-top: 1px solid #e3e3e3;
	padding-top: 25px;
	margin-top: 15px;
	display: inline-block;
	padding-left: 20px;
	padding-right: 20px;
	margin-right: -5px;
	margin-left: -5px;
}

.view-azalphabet span.azalphabet-letter {
	padding: 0px 9.5px;
	float: left;
	border-left: 1px solid #7a7a7a;
	line-height: 1.5;
}

.view-azalphabet span.azalphabet-letter:first-child {
	border-left: none;
}

.view-azalphabet span.azalphabet-letter > a {
	color: #333;
	font-size: 21px;
	line-height: 1;
}

.view-azalphabet span.azalphabet-letter > a:hover {
	color: #999 !important;
}

.view-azalphabet .azalphabet-most-viewed {
	background-color: #fff;
	padding: 20px 10px 30px;
	margin-top: 10px;
	display: inline-block;
}

.view-azalphabet .azalphabet-most-viewed > h2 {
	color: #62bda7;
	font-weight: 600;
	margin-top: 0px;
	border-bottom: 1px solid #e3e3e3;
	padding-bottom: 15px;
	padding-left: 10px;
	margin-right: 20px;
}

.view-azalphabet .azalphabet-most-viewed .azalphabet-mw-article {
	max-width: 215px;
	width: 100%;
	border-bottom: 1px solid #e3e3e3;
	padding: 5px 15px;
	margin-right: 45px;
}

.view-azalphabet .azalphabet-most-viewed .azalphabet-mw-article > a {
	font-size: 20px;
	color: #63857d;
	font-weight: 400;
}

.view-azalphabet .azalphabet-most-viewed .azalphabet-mw-article > a:hover {
	color: #62bda7;
}

.view-azalphabet .azalphabet-most-viewed .azalphabet-mw-article-wrapper {
	display: flex;
	flex-wrap: wrap;
}

.view-azalphabet .azalphabet-most-viewed .azalphabet-mw-article:nth-last-child(-n+3) {
	border-bottom: none;
}

.view-azalphabet .azalphabet-letter-result {
	background-color: #fff;
	padding: 20px 10px 35px;
	margin-top: 10px;
}

.view-azalphabet .azalphabet-letter-result > h3 {
	font-size: 42px;
	font-weight: 600;
	color: #454e4c;
	border-bottom: 1px solid #e3e3e3;
	padding-bottom: 15px;
	padding-left: 10px;
	margin-top: 0px;
	margin-right: 20px;
	margin-bottom: 15px;
}

.view-azalphabet .azalphabet-letter-result .azalphabet-lr-article-wrapper {
	display: flex;
	flex-wrap: wrap;
}

.view-azalphabet .azalphabet-letter-result .azalphabet-lr-article {
	max-width: 215px;
	width: 100%;
	border-bottom: 1px solid #e3e3e3;
	padding: 5px 15px 7px;
	margin-right: 45px;
}

.view-azalphabet .azalphabet-letter-result .azalphabet-lr-article:nth-last-child(-n+3) {
	border-bottom: none;
}

.view-azalphabet .azalphabet-letter-result .azalphabet-lr-article > a {
	color: #63857d;
	font-size: 16px;
	font-weight: 400;
}

.view-article .module.health-at-risk {
	display: none;
}

.view-healthaz .fb-side-module,
.view-azalphabet .fb-side-module {
	display: none;
}


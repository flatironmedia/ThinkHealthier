/* Health News */
/* Pagination */
.view-healthnews.layout-blog .pagination {
	text-align: center;
	margin: 20px 0px;
}

.view-healthnews.layout-blog .pagination > * {
	display: inline-block;
}

.view-healthnews.layout-blog .pagination .previous-page > a,
.view-healthnews.layout-blog .pagination .next-page > a {
	font-size: 0;
}

.view-healthnews.layout-blog .pagination .previous-page > span::before,
.view-healthnews.layout-blog .pagination .previous-page > a::before {
	content: "\f104";
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    text-decoration: inherit;
    color: #999999;
	font-size: 18px;
	width: 35px;
	height: 35px;
	border: 2px solid #999999;
	border-radius: 50%;
	display: inline-block;
	text-align: center;
	padding-top: 1px;
	margin-right: 20px;
}

.view-healthnews.layout-blog .pagination .next-page > span::before,
.view-healthnews.layout-blog .pagination .next-page > a::before {
	content: "\f105";
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    text-decoration: inherit;
    color: #999999;
	font-size: 18px;
	width: 35px;
	height: 35px;
	border: 2px solid #999999;
	border-radius: 50%;
	display: inline-block;
	text-align: center;
	padding-top: 1px;
	padding-left: 2px;
	margin-left: 20px;
}

.view-healthnews.layout-blog .pagination .previous-page > span::before,
.view-healthnews.layout-blog .pagination .next-page > span::before {
	cursor: not-allowed;
}

.view-healthnews.layout-blog .pagination .page-counter {
	font-size: 14px;
	font-family: 'Open Sans', sans-serif;
	color: #999;
	font-weight: 600;
}

.health-news-category-title {
	font-size: 36px;
	font-weight: normal;
	color: #64887e;
	line-height: 38px;
	border-bottom: 1px solid #c4c4c4;
	padding-bottom: 18px;
	margin-bottom: 15px;
}

.health-news footer#t3-footer {
	position: static !important;
	width: auto !important;
}

.health-news .t3-footer > .container {
	display: block !important;
}

.health-news .copyright {
	background: #fff;
	font-size: 14px;
	padding: 12px 0;
	text-align: center;
}

.health-news a#toggle-footer {
    display: none !important;
}

.view-healthnews.layout-blog .joomla_add_this {
	display: none !important;
}

.view-healthnews.layout-blog.health-news .t3-mainbody {
	background: #f2f2f2;
}

.view-healthnews.layout-blog.health-news .items-row .item {
	float: left;
	width: 100%;
	margin-bottom: 15px;
}

.view-healthnews.layout-blog.health-news article {
    position: relative;
    float: left;
    width: 100%;
}

.view-healthnews.layout-blog.health-news .article-intro {
    line-height: 1.5;
    position: absolute;
	top: 50%;
	transform: translateY(-50%);
	left: 265px;
	font-size: 14px;
	color: #6f847d;
}

.view-healthnews.layout-blog.health-news article .intro-image-wrap {
	margin: 0 15px 0 0;
}

.view-healthnews.layout-blog.health-news article .intro-image-wrap .item-image img {
	padding: 0;
}

.view-healthnews.layout-blog.health-news h2.article-title {
	font-size: 16px !important;
	line-height: normal !important;
	margin-bottom: 8px;
}

.view-healthnews.layout-blog.health-news .article-header a {
	font-size: 18px;
	font-weight: 500;
	color: #63857d;
}

.article-info .create,
.article-info .modified,
.article-info .published {
	padding: 0;
	line-height: 1;
	padding-bottom: 8px;
	font-size: 13px;
	color: #92a9a1;
}

.article-info .published {
	background: none;
	height: auto;
	width: auto;
	border-radius: 0;
}

.layout-blog .article-footer {
	border-top: 0px none;
	margin-top: 0px;
	padding-top: 0px;
}

.layout-blog .article-footer .readmore > a.readmore-link {
	text-transform: uppercase;
	font-size: 12px;
	color: #89c2b3;
}

.layout-blog .article-footer .readmore > a.readmore-link > span {
	margin-top: 8px;
	display: block;
}

.view-healthnews.layout-blog.health-news .article-bottom {
	padding-left: 10px;
}


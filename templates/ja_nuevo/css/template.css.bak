/* Styling RANGE input type */
input[type=range] {
  -webkit-appearance: none;
  margin: 20px 0 25px 0;
  width: 100%;
}
input[type=range]:focus {
  outline: none;
}
input[type=range]::-webkit-slider-runnable-track {
  width: 100%;
  height: 6px;
  cursor: pointer;
  animate: 0.2s;
  background: #63bea8;
}
input[type=range]::-webkit-slider-thumb {
  height: 33px;
  width: 48px;
  background: url(../images/range-slider-thumb.png) left bottom no-repeat;
  cursor: pointer;
  -webkit-appearance: none;
  border-radius: 0px;
  border: 0;
  margin-top: -15px;
}
input[type=range]:focus::-webkit-slider-runnable-track {
  background: #63bea8;
}
input[type=range]::-moz-range-track {
  width: 100%;
  height: 6px;
  cursor: pointer;
  animate: 0.2s;
  background: #63bea8;
}
input[type=range]::-moz-range-thumb {
  height: 33px;
  width: 49px;
  background: url(../images/range-slider-thumb.png) left bottom no-repeat;
  cursor: pointer;
  border-radius: 0px;
  border: 0;
}
input[type=range]::-ms-track {
  width: 100%;
  height: 6px;
  cursor: pointer;
  animate: 0.2s;
  color: transparent;
  border:0;
}
input[type=range]::-ms-fill-lower {
  background: #63bea8;
}
input[type=range]::-ms-fill-upper {
  background: #63bea8;
}
input[type=range]::-ms-thumb {
  height: 33px;
  width: 48px;
  background: url(../images/range-slider-thumb.png) left bottom no-repeat;
  cursor: pointer;
  border-radius: 0px;
  border: 0;
}
input[type=range]:focus::-ms-fill-lower {
  background: #63bea8;
}
input[type=range]:focus::-ms-fill-upper {
  background: #63bea8;
}
/* Styling RANGE input type */



